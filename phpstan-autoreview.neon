parameters:
	level: 9
	paths:
		- src
		- tests
	treatPhpDocTypesAsCertain: false
	tmpDir: var/cache/phpstan-autoreview
	excludePaths:
		- tests/Unit/PHPStan/Rules/Architecture/Fixtures/SingleClassPerFile
	ignoreErrors:
		-
			identifier: property.unused
			path: tests/Unit/PHPStan/Rules/Naming/Fixtures/TypeSuffixMismatch/*
		-
			identifier: property.onlyWritten
			path: tests/Unit/PHPStan/Rules/Naming/Fixtures/TypeSuffixMismatch/*
		-
			identifier: void.pure
			path: tests/Unit/PHPStan/Rules/Naming/Fixtures/TypeSuffixMismatch/*

# Experimental autoreview rules are added here in PR4+.
rules: []
